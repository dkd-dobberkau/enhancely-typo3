{
    "name": "enhancely/enhancely-for-typo3",
    "type": "typo3-cms-extension",
    "description": "Enhancely JSON-LD integration for TYPO3 - AI-generated structured data for SEO",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Enhancely",
            "email": "info@enhancely.ai"
        }
    ],
    "require": {
        "php": "^8.2",
        "typo3/cms-core": "^12.4 || ^13.0 || ^14.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Enhancely\\Enhancely\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enhancely\\Tests\\": "Tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || ^11.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "enhancely"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
